package com.example.logisticapp1;

import androidx.appcompat.app.AppCompatActivity;

import android.content.Intent;
import android.os.Bundle;
import android.view.View;

public class Distribuidor extends AppCompatActivity {

    @Override
    protected void onCreate(Bundle savedInstanceState) {
        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_distribuidor);
    }

    public void CambioClave (View view){
        Intent CambioClave = new Intent(this, CambioClave.class);
        startActivity(CambioClave);

    }

    public void Cerrar (View view){
        finish();

    }

    public void ResgitrarDespacho (View view){
        Intent ResgitrarDespacho = new Intent(this, ResgitrarDespacho.class);
        startActivity(ResgitrarDespacho);

    }

    public void Ayuda (View view){
        Intent Ayuda = new Intent(this, AyudaUser.class);
        startActivity(Ayuda);

    }
}

